<Window x:Class="CountessQuantaControl.RobotDisplay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RobotDisplay" Height="600" Width="600" Loaded="WindowLoaded">
    <Canvas Name="RobotDisplayCanvas">
        <GroupBox Canvas.Left="12" Canvas.Top="12" Header="Target Position" Height="105" Name="groupBox1" Width="117" FontSize="14">
            <Grid>
                <Label Content="X = " Height="28" HorizontalAlignment="Left" Margin="6,6,0,0" Name="targetXLabel" VerticalAlignment="Top" />
                <Label Content="Y = " Height="28" HorizontalAlignment="Left" Margin="6,27,0,0" Name="targetYLabel" VerticalAlignment="Top" />
                <Label Content="Z = " Height="28" HorizontalAlignment="Left" Margin="6,50,0,0" Name="targetZLabel" VerticalAlignment="Top" />
            </Grid>
        </GroupBox>
        <Label Canvas.Left="342" Canvas.Top="12" Content="Robot Neck Servo Position:" Height="28" Name="label1" FontSize="14" />
        <Label Canvas.Left="526" Canvas.Top="12" Content="1550" Height="28" Name="neckServoLabel" FontSize="14" />
    </Canvas>
</Window>
